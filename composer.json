    "type": "neos-plugin",
    "name": "shel/neos-subtrees",
    "license": "MIT",
    "require": {
        "neos/neos": "^7.3 || ^8.3"
    },
    "autoload": {
        "psr-4": {
            "Shel\\Neos\\SubTrees\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Shel.Neos.SubTrees"
        }
    }
}
